{
  "name": "rollup-typescript-boilerplate",
  "version": "0.1.0",
  "description": "Rollup Typescript Boilerplate",
  "main": "./dist/app.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "serve": "rollup -c config/rollup.config.dev.js -w --environment SERVE:true",
    "build:dev": "rollup -c config/rollup.config.dev.js",
    "build": "rollup -c config/rollup.config.prod.js",
    "lint": "yarn check-types && yarn eslint",
    "lint:fix": "yarn eslint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "check-types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haneulcho/rollup-typescript-boilerplate.git"
  },
  "author": "Haneul Cho",
  "license": "MIT",
  "homepage": "https://github.com/haneulcho/rollup-typescript-boilerplate",
  "_moduleAliases": {
    "@": "src"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@svgr/rollup": "^6.2.1",
    "@types/node": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "module-alias": "^2.2.2",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-browsersync": "^1.3.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "3",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "sass": "^1.49.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
